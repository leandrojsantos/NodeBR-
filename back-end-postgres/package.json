{
  "name": "back-end-postgres",
  "version": "6.0.1",
  "description": "hapi-api usando somente postgres como database",
  "private": true,
  "main": "api.js",
  "scripts": {
    "preinstall": "npm i -g pm2",
    "prod": "cross-env NODE_ENV=prod pm2-runtime api.js",
