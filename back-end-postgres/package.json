    "hapi-swagger": "10.0.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "hapi": "^18.1.0",
    "inert": "^5.1.3",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.9",
    "nanoid": "^2.1.9",
